            <plyta>
                <tytul>Małomiasteczkowy</tytul>
                <wykonawca>Podsiadło Dawid</wykonawca>
                <gatunek>Pop &amp; Rock</gatunek>
                <cena>36,99</cena>
                <ocena> 4,64 </ocena>
